<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/historicos_cardView"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_margin="5dp"
    android:padding="5dp">


    <ImageView
        android:id="@+id/item_persornagens_imagem"
        android:layout_width="80dp"
        android:layout_height="80dp"
        tools:src="@color/rosa"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>

    <TextView
        android:id="@+id/item_persornagens_textview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Teste"
        android:textSize="14sp"
        android:textAlignment="center"
        app:layout_constraintTop_toBottomOf="@id/item_persornagens_imagem"
        app:layout_constraintStart_toStartOf="@id/item_persornagens_imagem"
        app:layout_constraintEnd_toEndOf="@id/item_persornagens_imagem"/>


</androidx.constraintlayout.widget.ConstraintLayout>